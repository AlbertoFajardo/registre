        <class>es.caib.regweb.model.Oficina</class>
        <class>es.caib.regweb.model.Organismo</class>
        <class>es.caib.regweb.model.CatEstadoEntidad</class>
        <class>es.caib.regweb.model.RelacionOrganizativaOfi</class>
        <class>es.caib.regweb.model.RelacionSirOfi</class>
        <class>es.caib.regweb.model.Rol</class>
        <class>es.caib.regweb.model.PermisoLibroUsuario</class>
        <class>es.caib.regweb.model.TipoAsunto</class>
        <class>es.caib.regweb.model.CodigoAsunto</class>

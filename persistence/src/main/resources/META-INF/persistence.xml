        <class>es.caib.regweb.model.CatEntidadGeografica</class>
        <class>es.caib.regweb.model.CatLocalidad</class>
        <class>es.caib.regweb.model.RegistroEntrada</class>
        <class>es.caib.regweb.model.RegistroSalida</class>
        <class>es.caib.regweb.model.RegistroDetalle</class>
        <class>es.caib.regweb.model.ModeloRecibo</class>
        <class>es.caib.regweb.model.Archivo</class>
        <class>es.caib.regweb.model.Persona</class>
        <class>es.caib.regweb.model.Interesado</class>
